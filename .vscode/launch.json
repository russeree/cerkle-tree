    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug All Tests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/cerkle-tree_test",
            "args": [
                "--log_level=all",
                "--report_level=detailed",
                "--color_output=yes",
                "--show_progress=yes"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "debug-all-tests",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
